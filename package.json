{
  "name": "tdd-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start:dev": "cross-env NODE_ENV=production ts-node ./src/server",
    "start": "cross-env NODE_ENV=production node ./dist/server",
    "test": "cross-env npm run coverage && npm run lint",
    "coverage": "cross-env ENV_TEST=.env.test NODE_ENV=test jest",
    "test:unit": "cross-env LOG=true ENV_TEST=.env.test NODE_ENV=test jest",
    "test:dev": "cross-env LOG=true ENV_TEST=.env.test NODE_ENV=test jest --watch",
    "lint": "eslint --ext .js src",
    "lint:fix": "eslint --fix --ext .js|.ts src"
  },
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "custom-env": "^2.0.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "helmet": "^6.1.5",
    "lodash": "^4.17.21",
    "mongoose": "^7.1.0",
    "mongoose-connect": "0.0.3",
    "nock": "^13.3.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "jest-circus": "^29.5.0",
    "mongodb": "^5.3.0",
    "mongodb-memory-server": "^8.12.2",
    "np": "^7.7.0",
    "pre-push": "^0.1.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
